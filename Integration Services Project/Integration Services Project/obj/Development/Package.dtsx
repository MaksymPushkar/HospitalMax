                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    d.patient_id, 
    d.treating_doctor_id AS doctor_id, 
    d.disease_id, 
    m.medication_id, 
    d.diagnosis_date, 
    d.treatment_start_date, 
    ISNULL(d.treatment_end_date, GETDATE()) AS treatment_end_date, 
    ISNULL(d.rating, 1) as rating, 
    CASE 
        WHEN d.treatment_end_date IS NULL THEN 'Ongoing'
        WHEN d.rating &gt;= 85 THEN 'Successful'
        ELSE 'Unsuccessful'
    END AS treatment_status
FROM Diagnoses d
JOIN Medications m ON d.disease_id = m.medication_id
JOIN Patients p ON d.patient_id = p.patient_id
WHERE d.diagnosis_date IS NOT NULL 
  AND d.treatment_start_date IS NOT NULL
  AND NOT EXISTS (
      SELECT 1 
      FROM Patients p2 
      JOIN Diagnoses d2 ON p2.patient_id = d2.patient_id
      WHERE p2.patient_id = d.patient_id 
        AND (
            EXISTS (
                SELECT 1 
                FROM Medications m2 
                WHERE CHARINDEX(m2.medication_name, p2.contraindications) &gt; 0 
                  AND m2.medication_id = d2.disease_id
            ) 
            OR (
                SELECT COUNT(*) 
                FROM Diagnoses d3 
                WHERE d3.treating_doctor_id = d.treating_doctor_id 
                  AND d3.diagnosis_date = d.diagnosis_date
            ) &gt; 10
        )
  );</property>
